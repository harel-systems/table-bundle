{
    "name": "harel-systems/table-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to generate dynamic data tables",
    "keywords": ["table", "datagrid"],
    "homepage": "https://www.helloharel.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Corteel",
            "email": "maxime.corteel@harelsystems.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/security-bundle": ">=6.3",
        "symfony/dependency-injection": ">=6.3",
        "symfony/http-foundation": ">=6.3",
        "symfony/routing": ">=6.3",
        "symfony/event-dispatcher": ">=6.3",
        "doctrine/orm": ">=2.0"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {
            "Harel\\TableBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harel\\TableBundle\\Tests\\": "/Tests/"
        }
    }
}
